package example9_databox;

public class Databox {

	private String data;
	
	public synchronized String geDate() {
		if(this.data == null) {
			try {
				wait();
			}catch(Exception e) {
				
			}
		}
		String returnValue = data;
		System.out.println("consummerThread가 읽은 데이터 : " + returnValue);
		
		
		return "";
	}
	
	
	
}
